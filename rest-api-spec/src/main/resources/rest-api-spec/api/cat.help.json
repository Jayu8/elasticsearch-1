{
  "cat.help":{
    "documentation":{
      "url":"http://www.elastic.co/guide/en/elasticsearch/reference/master/cat.html"
    },
    "stability":"stable",
    "url":{
      "paths":[
        {
          "path":"/_cat",
          "methods":[
            "GET"
          ]
        }
      ]
    },
    "params":{
      "help":{
        "type":"boolean",
        "description":"Return help information",
        "default":false
      },
      "s":{
